<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="exchangeDashboard">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backButton" prefHeight="26.0" prefWidth="112.0" text="Back" />
      <Button layoutX="456.0" layoutY="350.0" mnemonicParsing="false" onAction="#signOutButton" prefHeight="26.0" prefWidth="112.0" text="Sign Out" />
      <TextField fx:id="amountField" layoutX="14.0" layoutY="60.0" promptText="Enter Amount" />
      <TableView fx:id="tableview" layoutX="26.0" layoutY="132.0" prefHeight="200.0" prefWidth="556.0">
        <columns>
          <TableColumn fx:id="amounttableView" prefWidth="176.0" text="Amount" />
          <TableColumn fx:id="currencyTableView" prefWidth="180.0" text="Currency" />
            <TableColumn fx:id="exchangeTableView" prefWidth="199.0" text="Exchange Rate" />
        </columns>
      </TableView>
      <ComboBox fx:id="currencyCombo" layoutX="205.0" layoutY="60.0" prefWidth="150.0" />
      <Button layoutX="421.0" layoutY="60.0" mnemonicParsing="false" onAction="#enterButton" prefHeight="26.0" prefWidth="112.0" text="Enter" />
   </children>
</AnchorPane>
